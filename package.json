{
  "name": "WhenWeWork",
  "description": "Build scripts for Heroku",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "npm install --prefix _client && npm run build --prefix _client",
    "clean": "rm -rf public",
    "deploy": "cp -a client/build/. public/",
    "heroku-postbuild": "npm install --prefix _client && npm run build --prefix _client"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "@syncfusion/ej2-react-schedule": "^20.4.53"
  }
}